#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :DemoPaper
                :FullPaper
                :Poster
                :ShortPaper
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :ExpertGroup
                :RegularConference
                :Symposium
                :Workshop
              )
] .



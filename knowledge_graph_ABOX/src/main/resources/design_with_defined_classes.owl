###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/AcceptedPaper
:AcceptedPaper rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :SubmittedPaper
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasPositiveReviewSubmitted ;
                                                            owl:allValuesFrom :PositiveReview
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Area
:Area rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :areaHasName ;
                        owl:someValuesFrom xsd:string
                      ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf :Person .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Chair
:Chair rdf:type owl:Class ;
       rdfs:subClassOf :Person .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Conference
:Conference rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Venue
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :handledByChair ;
                                                         owl:someValuesFrom :Chair
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            owl:disjointWith :Journal .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/ConferenceProceedings
:ConferenceProceedings rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty :conferenceProceedingsHasYear ;
                                         owl:someValuesFrom xsd:int
                                       ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/DemoPaper
:DemoPaper rdf:type owl:Class ;
           rdfs:subClassOf :Paper .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Editor
:Editor rdf:type owl:Class ;
        rdfs:subClassOf :Person .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/ExpertGroup
:ExpertGroup rdf:type owl:Class ;
             rdfs:subClassOf :Conference .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/FullPaper
:FullPaper rdf:type owl:Class ;
           rdfs:subClassOf :Paper .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Journal
:Journal rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Venue
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :handledByEditor ;
                                                      owl:someValuesFrom :Editor
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/JournalVolume
:JournalVolume rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :journalVolumeHasYear ;
                                 owl:someValuesFrom xsd:int
                               ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/NegativeReview
:NegativeReview rdf:type owl:Class ;
                rdfs:subClassOf :Review ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasNegativeDecision ;
                                  owl:someValuesFrom xsd:string
                                ] ;
                owl:disjointWith :PositiveReview .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Paper
:Paper rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasAuthor ;
                         owl:someValuesFrom :Author
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :isRelatedTo ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Area
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasTitle ;
                         owl:someValuesFrom xsd:string
                       ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :personHasName ;
                          owl:someValuesFrom xsd:string
                        ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/PositiveReview
:PositiveReview rdf:type owl:Class ;
                rdfs:subClassOf :Review ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasPositiveDecision ;
                                  owl:someValuesFrom xsd:string
                                ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Poster
:Poster rdf:type owl:Class ;
        rdfs:subClassOf :Paper ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :isSubmitted ;
                          owl:allValuesFrom :Conference
                        ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/RegularConference
:RegularConference rdf:type owl:Class ;
                   rdfs:subClassOf :Conference .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Review
:Review rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :NegativeReview
                                            :PositiveReview
                                          )
                            ] ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasText ;
                          owl:someValuesFrom xsd:string
                        ] ;
        owl:disjointWith :Venue .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Reviewer
:Reviewer rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :submitsReview ;
                                                       owl:someValuesFrom :Review
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/ShortPaper
:ShortPaper rdf:type owl:Class ;
            rdfs:subClassOf :Paper .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/SubmittedPaper
:SubmittedPaper rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Paper
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :isSubmitted ;
                                                             owl:someValuesFrom :Venue
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasReviewer ;
                                                             owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                             owl:onClass :Reviewer
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Symposium
:Symposium rdf:type owl:Class ;
           rdfs:subClassOf :Conference .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Venue
:Venue rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:unionOf ( :Conference
                                           :Journal
                                         )
                           ] ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :isRelatedTo ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Area
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :venueHasName ;
                         owl:someValuesFrom xsd:string
                       ] .


###  http://www.semanticweb.org/odykypar/ontologies/2023/4/sdm-final/Workshop
:Workshop rdf:type owl:Class ;
          rdfs:subClassOf :Conference .


